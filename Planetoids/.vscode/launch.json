{
    "configurations": [
       
        {
            "name": "Debugh",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/linux/planets.out",
            "args": ["${workspaceFolder}/systems/tauri2000.csv"],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": false
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "MakeDebug"
        },
        {
            "name": "RUN",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/linux/planets.out",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "MakeRun"
        }
    ],
    "version": "2.0.0"
}